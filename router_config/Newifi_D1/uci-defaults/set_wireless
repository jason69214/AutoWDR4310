#!/bin/sh
uci set wireless.@wifi-device[0].country='CN'
uci set wireless.@wifi-device[0].htmode='HT40'
uci set wireless.@wifi-device[0].channel='6'
uci set wireless.@wifi-device[0].noscan='1'
uci set wireless.@wifi-device[1].country='CN'
uci set wireless.@wifi-device[1].htmode='VHT80'
uci set wireless.@wifi-device[1].channel='149'
uci set wireless.@wifi-iface[0].ssid='Newifi_D1_2.4G'
uci set wireless.@wifi-iface[0].encryption='psk-mixed+ccmp'
uci set wireless.@wifi-iface[0].key='123456aA'
uci set wireless.@wifi-iface[1].ssid='Newifi_D1_5G'
uci set wireless.@wifi-iface[1].encryption='psk-mixed+ccmp'
uci set wireless.@wifi-iface[1].key='123456aA'
uci set wireless.@wifi-iface[1].ieee80211r='1'
uci set wireless.@wifi-iface[1].mobility_domain='1221'
uci set wireless.@wifi-iface[1].reassociation_deadline='20000'
uci set wireless.@wifi-iface[1].ft_over_ds='0'
uci set wireless.@wifi-iface[1].ft_psk_generate_local='1'
uci set wireless.@wifi-iface[1].bss_transition='1'
uci set wireless.@wifi-iface[1].wnm_sleep_mode='1'
uci set wireless.@wifi-iface[1].time_advertisement='2'
uci set wireless.@wifi-iface[1].ieee80211k='1'
uci set wireless.@wifi-iface[1].rrm_neighbor_report='1'
uci set wireless.@wifi-iface[1].rrm_beacon_report='1'
uci set network.wan.peerdns='0'
uci add_list network.wan.dns='223.5.5.5'
uci add_list network.wan.dns='114.114.114.114'
uci set system.@system[-1].zonename='Asia/Shanghai'
uci set system.@system[-1].timezone='CST-8'
uci set system.@system[-1].hostname='Newifi-D1'
uci commit
/etc/init.d/network restart
/etc/init.d/system restart
exit 0
